<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service xmlns="http://www.activegrid.com/namespaces/ServiceDefinitions/1.0" id="UserDatabase" type="DataService" springFile="UserDatabase.spring.xml">
    <dataobjects>
        <dataobject javaType="com.bccontrolpanel.userdatabase.Users">
            <element name="id" typeRef="java.lang.Integer" allowNull="true"/>
            <element name="username" typeRef="java.lang.String" allowNull="true"/>
            <element name="role" typeRef="java.lang.Integer" allowNull="true"/>
            <element name="password" typeRef="java.lang.String" allowNull="true"/>
        </dataobject>
        <dataobject javaType="com.bccontrolpanel.userdatabase.Roles">
            <element name="id" typeRef="java.lang.Integer" allowNull="true"/>
            <element name="roleDescription" typeRef="java.lang.String" allowNull="true"/>
        </dataobject>
    </dataobjects>
</service>
