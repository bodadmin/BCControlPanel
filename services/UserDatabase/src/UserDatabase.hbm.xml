<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bccontrolpanel.userdatabase">
  <class table="`ROLES`" entity-name="com.bccontrolpanel.userdatabase.Roles" schema="UserDatabase">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="roleDescription" type="string">
      <column name="`RoleDescription`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
  <class table="`USERS`" entity-name="com.bccontrolpanel.userdatabase.Users" schema="UserDatabase">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="password" type="string">
      <column name="`PASSWORD`" length="255" not-null="true" precision="19"/>
    </property>
    <property name="username" type="string">
      <column name="`USERNAME`" length="255" not-null="true" precision="19"/>
    </property>
    <property name="role" type="integer">
      <column name="`ROLE`" length="255" not-null="true" precision="19"/>
    </property>
  </class>
  <class table="`MENUS`" entity-name="com.bccontrolpanel.userdatabase.Menus" schema="UserDatabase">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="menuId" type="integer">
      <column name="`MenuID`" length="255" not-null="false" precision="19" default="'0'"/>
    </property>
    <property name="menuItemName" type="string">
      <column name="`MenuItemName`" length="40" not-null="true" precision="19"/>
    </property>
    <property name="menuItemLink" type="string">
      <column name="`MenuItemLink`" length="255" not-null="false" precision="19"/>
    </property>
    <property name="menuItemIcon" type="string">
      <column name="`MenuItemIcon`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
</hibernate-mapping>