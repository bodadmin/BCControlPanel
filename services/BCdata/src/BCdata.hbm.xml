<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.bccontrolpanel.bcdata">
  <class table="`users`" entity-name="com.bccontrolpanel.bcdata.Users" schema="BCdata">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="userrname" type="string">
      <column name="`userrname`" length="255" not-null="false" precision="19"/>
    </property>
    <property name="password" type="string">
      <column name="`password`" length="255" not-null="true" precision="19"/>
    </property>
    <property name="role" type="string">
      <column name="`role`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
  <class table="`role`" entity-name="com.bccontrolpanel.bcdata.Role" schema="BCdata">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="role" type="string">
      <column name="`role`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
  <class table="`contextfiles`" entity-name="com.bccontrolpanel.bcdata.Contextfiles" schema="BCdata">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="assigned"/>
    </id>
    <property name="description" type="string">
      <column name="`description`" length="255" not-null="false" precision="19"/>
    </property>
    <property name="filename" type="string">
      <column name="`filename`" length="255" not-null="true" precision="19"/>
    </property>
    <property name="environment" type="string">
      <column name="`environment`" length="5" not-null="true" precision="19" default="'Dev'"/>
    </property>
  </class>
  <class table="`menusettings`" entity-name="com.bccontrolpanel.bcdata.Menusettings" schema="BCdata">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="menuItemLabel" type="string">
      <column name="`menuItemLabel`" length="255" not-null="true" precision="19"/>
    </property>
    <property name="menuItemLink" type="string">
      <column name="`menuItemLink`" length="255" not-null="true" precision="19"/>
    </property>
    <property name="menuItemIcon" type="string">
      <column name="`menuItemIcon`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
  <class table="`context`" entity-name="com.bccontrolpanel.bcdata.Context" schema="BCdata">
    <id name="id" type="integer">
      <column name="`ID`" length="255" not-null="true" precision="19"/>
      <generator class="identity"/>
    </id>
    <property name="contextname" type="string">
      <column name="`contextname`" length="100" not-null="true" precision="19"/>
    </property>
    <property name="contextvalue" type="string">
      <column name="`contextvalue`" length="100" not-null="false" precision="19"/>
    </property>
    <property name="contextenabled" type="boolean">
      <column name="`contextenabled`" length="255" not-null="true" precision="19" default="false"/>
    </property>
    <property name="contextId" type="integer">
      <column name="`contextID`" length="255" not-null="true" precision="19" default="'0'"/>
    </property>
  </class>
</hibernate-mapping>